@model  List<ProyectoMatricula.Modelos.pa_Cuatrimestre_Select_Result>
@{
    ViewBag.Title = "Matrícula UMCA | Lista de cuatrimestres";
    Layout = "~/Views/Shared/_LayoutMatricula.cshtml";
}

<h1 class="text-center">Lista de cuatrimestres</h1>
<form class="d-flex align-content-center justify-content-center" method="post">
    <label class="form-check-label" id="txtCuatrimestre">Cuatrimestre</label>&nbsp;
    <input type="text" value="" id="Numero_Cuatrimestre" name="Numero_Cuatrimestre" class="form-control"/>
    &nbsp;
    <label class="form-check-label" id="lblSedeUniversitaria">Sede universitaria</label>&nbsp;
    <input type="text" value="" id="Nombre_Sede" name="Nombre_Sede" class="form-control"/>
    &nbsp;
    <label class="form-check-label" id="anio">Año de cuatrimestre</label>&nbsp;
    <input type="text" value="" class="form-control" name="Anio_Cuatrimestre" id="Anio_Cuatrimestre" />&nbsp;&nbsp;
    <button type="submit" class="btn btn-primary"><i class="fa fa-search" aria-hidden="true"></i>Buscar</button>
</form>
<br />
<table class="table text-center table-hover table-bordered table-striped">
    <tr>
        <td>Identificador</td>
        <td>Cuatrimestre</td>
        <td>Año</td>
        <td>Sede</td>
        <td>Inicio de cuatrimestre</td>
        <td>Fin de cuatrimestre</td>
    </tr>
    @{
        foreach (ProyectoMatricula.Modelos.pa_Cuatrimestre_Select_Result posicionModelo in this.Model)
        {
            <tr>
                <td>@posicionModelo.Id_Cuatrimeste</td>
                <td>@posicionModelo.Numero_Cuatrimestre</td>
                <td>@posicionModelo.Anio_Cuatrimestre</td>
                <td>@posicionModelo.Nombre_Sede</td>
                <td>@posicionModelo.Inicio_Cuatrimestre</td>
                <td>@posicionModelo.Fin_Cuatrimestre</td>
            </tr>
        }
    }
</table>

